package day3;

public class Interns extends Employees{
	private String report;

	public Interns(String name,int id,int salary,String report) {

	super(name,id,salary);
	this.report = report;
	 }
	void display() {
		System.out.println("Interns " );
		super.diaplay();
		System.out.println("report :" + report);
	}

}
